}
.cleanShop{
  color: #545454d4;
  height: 74rpx;
  border-bottom: 2rpx solid #dadada59;
  margin-bottom: 30rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  padding: 0rpx 30rpx;
  font-size: 22rpx;
}
.buy{
  width: 100%;
  height: 100px;
  width: 100%;
  padding: 0rpx 30rpx;
}
.buyImg{
  width: 130rpx;
  height: 130rpx;
}
.buyImg image{
  width: 100%;
  height: 100%;
}
.buyContent{
  display: flex;
  flex-direction: column;
  line-height: 72rpx;
  padding-left:15rpx;
  width: 565rpx;
  height: 81%;
}
.buyName{
  font-weight: 550;
  width: 100%;
  font-size: 30rpx;
}
.buyPrice{
  color: #ee5527;
  font-weight: 550;
  font-size: 38rpx;
}
.buyNumber{
  position: relative;
  bottom: 60rpx;
  float: right;
  width: 174rpx;
  height: 48rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.numberAdd{
  background-color: #FECC53;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 62rpx;
  height: 100%;
  margin:0 7rpx;
  border-bottom-right-radius: 8rpx;
  border-top-right-radius: 8rpx;
  color: #000;
}
.numberDel{
  /* background-color: rgba(179, 179, 179, 0.14); */
  display: flex;
  align-items: center;
  justify-content: center;
  width: 62rpx;
  height: 100%;
  margin:0 7rpx;
  border-bottom-left-radius: 8rpx;
  border-top-left-radius: 8rpx;
  color: #000;
  border: 2rpx solid #FECC53;
}
.numberShow{
  background-color: rgba(179, 179, 179, 0.14);
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  width: 70rpx;
  height: 100%;
}
/* .payShow{
  animation: payShow 0.5s;
}
.payHide{
  animation: payHide 0.5s;
} */
@keyframes payShow {
  0% {
      opacity: 0;
      transform: translateY(100%);
  }
  100% {
      opacity: 1;
      transform: translateY(0%);
  }
}
@keyframes payHide {
  0% {
      opacity: 1;
      transform: translateY(0%);
  }
  100% {
      opacity: 0;
      transform: translateY(100%);
  }
}










