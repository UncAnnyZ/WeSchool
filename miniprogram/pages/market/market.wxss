  align-items: center;
  font-weight: 550;
  font-size: 30rpx;
  height: 100%;
}
.EditCom,
.EditCom_background{
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 3;
  font-family: unset;
  display: flex;
  align-items: center;
  justify-content: center;
}
.EditCom_background{
  background-color: rgba(0, 0, 0, 0.92);
